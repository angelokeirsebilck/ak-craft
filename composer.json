{
  "require": {
    "craftcms/cms": "^3.7.26",
    "craftcms/redactor": "^2.10.6",
    "nystudio107/craft-imageoptimize": "1.6.44",
    "nystudio107/craft-seomatic": "^3.4.29",
    "vaersaagod/dospaces": "^1.1",
    "verbb/image-resizer": "2.1.1",
    "verbb/super-table": "^2.7.1",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "custommodule\\": "modules/custom-module/src"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
