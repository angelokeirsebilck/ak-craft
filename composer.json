{
  "require": {
    "craftcms/cms": "^4.0.0",
    "craftcms/redactor": "^3.0.0",
    "nystudio107/craft-imageoptimize": "^4.0.0",
    "nystudio107/craft-seomatic": "^4.0.1",
    "vaersaagod/dospaces": "^2.0.0",
    "verbb/image-resizer": "^3.0.0",
    "verbb/super-table": "^3.0.0",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "custommodule\\": "modules/custom-module/src"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
