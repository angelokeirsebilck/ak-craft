{% extends '_layout/_base.twig' %}

{% set entry = craft.entries().slug('500').one() %}

{% block content %}
    {% include '_inc/_title' with {
        title: entry.title
    } %}
    {% set content = entry.fieldAkContent.all() %}404{% for block in content %}
        {% include "/_blocks/_#{block.type.handle|replace('type', '')|lower}.twig" with {
            block: block
        } %}
    {% endfor %}
{% endblock %}
