{% extends '_layout/_base' %}
{# {% set content = entry.fieldContentBuilder.all() %} #}

{% set homeBanner = entry.fieldAkHomeBanner.one() %}
{% set sizes =
    '(max-width: 375px) 222px, (max-width: 640px) 400px, (max-width: 768px) 484px, (max-width: 1024px) 508px, (max-width: 1280px) 509px, 576px'
%}
{% block preload %}
    {# <link rel="preload" as="image" href="/img/agkiz-8o5wp.webp"> #}
    {% if homeBanner.itemPortrait.one() %}
        {% set optimizedImage =
            homeBanner.itemPortrait.one().fieldHomePortraitImgOpt
        %}

        <link rel="preload"
            as="image"
            type="image/webp"
            imagesrcset="{{ optimizedImage.srcsetWebP() }}"
            imagesizes="{{ sizes }}" />
    {% endif %}
{% endblock %}

{% block content %}
    {% include '_blocks/_home/_homeBanner.twig' with {
        block: homeBanner,
        sizes: sizes
    } %}

